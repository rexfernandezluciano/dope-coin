{
	"compilerOptions": {
		"incremental": true,
		"tsBuildInfoFile": "./node_modules/.tsbuildinfo",
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"target": "ES2022",
		"strict": true,
		"lib": ["esnext"],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"composite": true,
		"sourceMap": false,
		"allowSyntheticDefaultImports": true,
		"types": ["node", "vite/client"]
	}
}
